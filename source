newtype miorecord record(miamatrice int, floating float);

func prova(integer a, floating b): miamatrice
begin
	newvars integer a;
	a = 2;
	b();
end

func b(): null
begin
end

overload(+, mioarray)
begin

end


newvars miamatrice mat;
newvars mioarray arra;
newvars miorecord acc;


func exec (integer x) :  integer
begin
	mat = new(miamatrice);
	arra = new(mioarray);
	acc = new(miorecord);
	mat[1,3] = 1; 
end
