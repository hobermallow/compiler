newtype miamatrice matrix(integer, 4, 5);
newtype mioarray array(integer, 3, 4);
newtype miorecord record(integer int, floating float);

newvars floating z, d;
newvars mioarray arra;
newvars miamatrice mat;
newvars miorecord acc;

func exec (integer x) : integer 
begin
	mat = new(miamatrice);
	arra = new(mioarray);
	acc = new(miorecord);
	arra[2][2] = 1;
	mat[1,3] = 1; 
	acc->int = 3;
end
