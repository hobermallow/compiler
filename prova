newtype miorecord record(integer x);
newtype miamatrice matrix(integer, 3, 4 );
newtype miamatricee matrix(integer, 3, 4 );
newtype mioarray array(miamatrice, 4);
newtype mioarrayy array(miamatrice, 5);


newvars mioarray arra;
newvars mioarrayy arraa;
newvars miorecord rec;
newvars miorecord rec2;
newvars miamatricee mat;
newvars string ciao;
newvars char c;

func exec () :  integer
begin
	newvars integer x;
	printf("%d", x);
	rec = new(miorecord);
	rec2 = new(miorecord);
	arra = new(mioarray);
	arraa = new(mioarrayy);
	mat = new(miamatricee);
	scanf("%d", &mat[1,1]);
	printf("ciao %d\n", mat[1,1]);
	arra[0] = mat;
	if(mat[1,1] == 1)
	begin 
		printf("uguale a 1\n");
	end
	if(mat[1,1] == 1)
	begin 
		printf("uguale a 1\n");
	end
	loop(1 )
	begin
		x = 1;
		printf("cioa\n");
	end
	rec->x = 1;
	arra = arraa;
end
