newtype miorecord record(integer x);
newtype miamatrice matrix(integer, 3, 4 );
newtype mioarray array(miamatrice, 3, 5, 4, 6);

func ciao() : integer
begin
	return 1;
end

newvars mioarray arra;
newvars miorecord rec, rec2;
newvars miamatrice mat;

func exec () :  integer
begin
	newvars integer x;
	printf("%d", x);
	scanf("%d", &x);
	rec = new(miorecord);
	rec2 = new(miorecord);
	arra = new(mioarray);
	mat = new(miamatrice);
	mat[1,1] = 3;
	printf("ciao\n");
end
