newtype miorecord record(integer x);
newtype miamatrice matrix(integer, 3, 4 );
newtype mioarray array(miamatrice, 3);

func ciao() : integer
begin
	return 1;
end

newvars mioarray arra;
newvars miorecord rec, rec2;
newvars miamatrice mat;
newvars string ciao;
newvars char c;

func exec () :  integer
begin
	newvars integer x;
	printf("%d", x);
	rec = new(miorecord);
	rec2 = new(miorecord);
	arra = new(mioarray);
	arra[1] = new(miamatrice);
	mat = new(miamatrice);
	scanf("%d", &mat[1,1]);
	printf("ciao %d\n", mat[1,1]);
	if(mat[1,1] == 1)
	begin 
		printf("uguale a 1\n");
	end
	loop(1 )
	begin
		x = 1;
		printf("cioa\n");
	end
end
